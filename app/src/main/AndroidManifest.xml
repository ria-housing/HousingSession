<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/church"
        android:label="@string/app_name"
        android:roundIcon="@drawable/church"
        android:supportsRtl="true"
        android:theme="@style/Theme.HousingSession"
        tools:targetApi="31">
        <activity
            android:name=".WidgetsPract"
            android:exported="true"
            android:label="@string/title_activity_widgets_pract"
            android:theme="@style/Theme.HousingSession" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LazyComp"
            android:exported="false"
            android:label="@string/title_activity_lazy_comp"
            android:theme="@style/Theme.HousingSession" />
        <activity
            android:name=".CalcPrac"
            android:exported="false"
            android:label="@string/title_activity_calc_prac"
            android:theme="@style/Theme.HousingSession" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.HousingSession">

        </activity>
    </application>

</manifest>